{
  "@context": {
    "goos": "https://www.ocean-ops.org/goosmeta#",
    "schema": "https://schema.org/",

    "PlatformModel": "goos:PlatformModel",

    "id": {
      "@id": "goos:platformModelId",
      "@type": "xsd:string"
    },
    "name": {
      "@id": "goos:platformModelName",
      "@type": "xsd:string"
    },
    "description": {
      "@id": "goos:platformModelDescription",
      "@type": "xsd:string"
    },
    "type": {
      "@id": "goos:belongsToType",
      "@type": "@id"
    },
    "manufacturer": {
      "@id": "goos:hasManufacturer",
      "@type": "@id"
    }
  }
}
