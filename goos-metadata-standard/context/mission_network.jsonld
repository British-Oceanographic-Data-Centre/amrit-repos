{
  "@context": {
    "goos": "https://www.ocean-ops.org/goosmeta#",
    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "MissionNetwork": "goos:MissionNetwork",
    "id": {
      "@id": "goos:missionNetworkId",
      "@type": "xsd:string"
    },
    "name": {
      "@id": "goos:missionNetworkName",
      "@type": "xsd:string"
    },
    "description": {
      "@id": "goos:missionNetworkDescription",
      "@type": "xsd:string"
    },
    "parentSystem": {
      "@id": "goos:hasParentSystem",
      "@type": "@id"
    },
    "mission": {
      "@id": "goos:hasMission",
      "@type": "@id"
    }
  }
}
