{
  "@context": {
    "goos": "https://www.ocean-ops.org/goosmeta#",
    "sosa": "http://www.w3.org/ns/sosa/",
    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "Observation": "goos:Observation",
    "variable": {
      "@id": "goos:observedVariable",
      "@type": "@id"
    },
    "date": {
      "@id": "goos:observationDate",
      "@type": "xsd:dateTime"
    },
    "location": {
      "@id": "schema:location",
      "@type": "@id"
    },
    "sensor": {
      "@id": "goos:madeBySensor",
      "@type": "@id"
    },
    "dataURL": {
      "@id": "goos:dataURL",
      "@type": "@id"
    }
  }
}
