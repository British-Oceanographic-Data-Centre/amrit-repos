{
  "@context": {
    "goos": "https://www.ocean-ops.org/goosmeta#",
    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "StatusEvent": "goos:StatusEvent",
    "id": {
      "@id": "goos:id",
      "@type": "xsd:string"
    },
    "type": {
      "@id": "goos:statusType",
      "@type": "@id"
    },
    "date": {
      "@id": "goos:statusDate",
      "@type": "xsd:dateTime"
    },
    "comment": {
      "@id": "goos:statusComment",
      "@type": "xsd:string"
    },
    "assignedBy": {
      "@id": "goos:assignedBy",
      "@type": "@id"
    }
  }
}
