			{ "code": "SEN-01", "title": "id", "type": "string" },
			{ "code": "SEN-02", "title": "serialNumber", "type": "string" },
			{ "code": "SEN-03", "title": "model", "type": "SMD" }
		]
		},
		{
		"id": "SMD",
		"label": "Sensor Model",
