{
	"classes": [
		{
		"id": "MIS",
		"label": "GOOS Mission",
		"definition": "A mission representing the operational deployment of a platform for ocean observation.",
		"fields": [
			{ "code": "MIS-01", "title": "id", "type": "string" },
			{ "code": "MIS-02", "title": "wigosId", "type": "string" },
			{ "code": "MIS-03", "title": "startTime", "type": "date" },
			{ "code": "MIS-04", "title": "endTime", "type": "date" },
			{ "code": "MIS-05", "title": "instrument", "type": "PLA" },
			{ "code": "MIS-06", "title": "provider", "type": "WPR" },
			{ "code": "MIS-07", "title": "result", "type": "OBS[ ]" },
			{ "code": "MIS-08", "title": "status", "type": "STS" },
			{ "code": "MIS-09", "title": "activation", "type": "ACT" },
			{ "code": "MIS-10", "title": "sensorSetup", "type": "VSP[ ]" }
		]
		},
		{
		"id": "ACT",
		"label": "Activation",
		"definition": "The act of releasing or enabling a platform to begin mission of data collection and transmission at sea.",
		"fields": [
			{ "code": "ACT-01", "title": "id", "type": "string" },
			{ "code": "ACT-02", "title": "startTime", "type": "date" },
			{ "code": "ACT-03", "title": "location", "type": "PLC" },
			{ "code": "ACT-04", "title": "ship", "type": "SHP" }
		]
		},
		{
		"id": "DEA",
		"label": "Deactivation",
		"definition": "The act of retrieving or deactivating a platform to end its mission.",
		"fields": [
			{ "code": "DEA-01", "title": "id", "type": "string" },
			{ "code": "DEA-02", "title": "startTime", "type": "date" },
			{ "code": "DEA-03", "title": "location", "type": "PLC" },
			{ "code": "DEA-04", "title": "ship", "type": "SHP" }
		]
		},
		{
		"id": "CNT",
		"label": "Country",
		"definition": "United Nations member state or a group of states such as European Union.",
		"fields": [
			{ "code": "CNT-01", "title": "id", "type": "string" },
			{ "code": "CNT-02", "title": "code2", "type": "string" },
			{ "code": "CNT-03", "title": "name", "type": "string" }
		]
		},
		{
		"id": "OBS",
		"label": "Observation",
		"definition": "An observation recording a variable at a location and time, made by a sensor.",
		"fields": [
			{ "code": "OBS-01", "title": "id", "type": "string" },
			{ "code": "OBS-02", "title": "variable", "type": "VAR" },
			{ "code": "OBS-03", "title": "date", "type": "date" },
			{ "code": "OBS-04", "title": "location", "type": "PLC" },
			{ "code": "OBS-05", "title": "sensor", "type": "SEN" },
			{ "code": "OBS-06", "title": "dataURL", "type": "uri" }
		]
		},
		{
		"id": "VSP",
		"label": "Sensor Setup",
		"definition": "An installation and configuration of a sensor on a platform during a mission, to observe a specific variable. One setup = one sensor + one variable.",
		"fields": [
			{ "code": "VSP-01", "title": "id", "type": "string" },
			{ "code": "VSP-02", "title": "observesVariable", "type": "VAR" },
			{ "code": "VSP-03", "title": "sensor", "type": "SEN" },
			{ "code": "VSP-04", "title": "platform", "type": "PLA" },
			{ "code": "VSP-05", "title": "mission", "type": "MIS" },
			{ "code": "VSP-06", "title": "startTime", "type": "date" },
			{ "code": "VSP-07", "title": "endTime", "type": "date" }
		]
		},
		{
		"id": "PLC",
		"label": "Location",
		"definition": "A place and its geographic coordinates, used for referencing the position of an observation or event.",
		"fields": [
			{ "code": "PLC-01", "title": "latitude", "type": "number" },
			{ "code": "PLC-02", "title": "longitude", "type": "number" },
			{ "code": "PLC-03", "title": "elevation", "type": "number" }
		]
		},
		{
		"id": "STS",
		"label": "Status Type",
		"definition": "A controlled concept representing a status in the lifecycle of a mission or platform.",
		"fields": [
			{ "code": "STS-01", "title": "id", "type": "string" },
			{ "code": "STS-02", "title": "prefLabel", "type": "string" },
			{ "code": "STS-03", "title": "definition", "type": "string" }
		]
		},
		{
		"id": "LOG",
		"label": "Mission Log",
		"definition": "A log containing records of updates, status changes, and quality control events associated with a mission.",
		"fields": [
			{ "code": "LOG-01", "title": "id", "type": "string" },
			{ "code": "LOG-02", "title": "statusHistory", "type": "STE[ ]" },
			{ "code": "LOG-03", "title": "updateEntry", "type": "UPD[ ]" },
			{ "code": "LOG-04", "title": "qualityCheck", "type": "QCE[ ]" }
		]
		},
		{
		"id": "NET",
		"label": "Mission Network",
		"definition": "A virtual or thematic grouping of missions and platforms, often representing regional arrays, shared scientific goals, or collaborative initiatives.",
		"fields": [
			{ "code": "NET-01", "title": "id", "type": "string" },
			{ "code": "NET-02", "title": "name", "type": "string" },
			{ "code": "NET-03", "title": "description", "type": "string" },
			{ "code": "NET-04", "title": "parentSystem", "type": "SYS" },
			{ "code": "NET-05", "title": "mission", "type": "MIS[ ]" }
		]
		},
		{
		"id": "SYS",
		"label": "Observing System",
		"definition": "One of the high-level GOOS/WIGOS observing systems. These systems group together ocean observing missions by instrumentation or operational framework.",
		"fields": [
			{ "code": "SYS-01", "title": "id", "type": "string" },
			{ "code": "SYS-02", "title": "name", "type": "string" },
			{ "code": "SYS-03", "title": "description", "type": "string" }
		]
		},
		{
		"id": "WPR",
		"label": "WIGOS Program",
		"definition": "A country-affiliated or institutional entity contributing to WIGOS through the execution of ocean observation missions and platform operations.",
		"fields": [
			{ "code": "WPR-01", "title": "id", "type": "string" },
			{ "code": "WPR-02", "title": "name", "type": "string" },
			{ "code": "WPR-03", "title": "description", "type": "string" },
			{ "code": "WPR-04", "title": "country", "type": "CNT" },
			{ "code": "WPR-05", "title": "organizationContribution", "type": "ORG" }
		]
		},
		{
		"id": "CON",
		"label": "Organization Contribution",
		"definition": "A specific contribution by an organization to either a GOOS Mission or a WIGOS Program, playing a defined role.",
		"fields": [
			{ "code": "CON-01", "title": "id", "type": "string" },
			{ "code": "CON-02", "title": "organization", "type": "ORG" },
			{ "code": "CON-03", "title": "targetProgram", "type": "WPR" },
			{ "code": "CON-04", "title": "targetMission", "type": "MIS" },
			{ "code": "CON-05", "title": "role", "type": "ROL" },
			{ "code": "CON-06", "title": "description", "type": "string" },
			{ "code": "CON-07", "title": "startDate", "type": "date" },
			{ "code": "CON-08", "title": "endDate", "type": "date" }
		]
		},
		{
		"id": "ROL",
		"label": "Organization Role",
		"definition": "A controlled vocabulary of roles that organizations may play within a WIGOS or GOOS program context (e.g., funder, operator, DAC).",
		"fields": [
			{ "code": "ROL-01", "title": "id", "type": "string" },
			{ "code": "ROL-02", "title": "name", "type": "string" },
			{ "code": "ROL-03", "title": "description", "type": "string" }
		]
		},
		{
		"id": "ORG",
		"label": "Organization",
		"definition": "Any entity (public, private, institutional, or commercial) involved in ocean observation activities, including agencies, manufacturers, operators, and funders.",
		"fields": [
			{ "code": "ORG-01", "title": "id", "type": "string" },
			{ "code": "ORG-02", "title": "name", "type": "string" },
			{ "code": "ORG-03", "title": "description", "type": "string" }
		]
		},
		{
		"id": "SHP",
		"label": "Ship",
		"definition": "A ship that deployed the platform, or served as the platform's installation or operation base.",
		"fields": [
			{ "code": "SHP-01", "title": "id", "type": "string" },
			{ "code": "SHP-02", "title": "name", "type": "string" },
