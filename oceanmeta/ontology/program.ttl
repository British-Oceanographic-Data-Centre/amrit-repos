@prefix goos: <https://www.ocean-ops.org/goos#> .
@prefix schema: <https://schema.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
#    Program Class
#################################################################

goos:Program a owl:Class ;
    rdfs:label "Program"@en ;
    rdfs:comment "Structured collaboration group of independant organisations with distinct roles."@en ;
    rdfs:subClassOf schema:Organization .

#################################################################
#    Properties
#################################################################

# Program ID
goos:programId a owl:DatatypeProperty ;
    rdfs:domain goos:Program ;
    rdfs:range xsd:string ;
    rdfs:label "Program ID"@en ;
    rdfs:comment "Internal identifier for the program."@en .

# Program Name
goos:programName a owl:DatatypeProperty ;
    rdfs:subPropertyOf schema:name ;
    rdfs:domain goos:Program ;
    rdfs:range xsd:string ;
    rdfs:label "Program Name"@en ;
    rdfs:comment "Full official name of the program."@en .

# Associated Country
goos:hasCountry a owl:ObjectProperty ;
    rdfs:domain goos:Program ;
    rdfs:range goos:Country ;
    rdfs:label "Country"@en ;
    rdfs:comment "The country where the program is based or primarily operates."@en .
